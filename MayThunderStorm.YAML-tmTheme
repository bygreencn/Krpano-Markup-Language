- name: docstring
  scope: string.quoted.double.block, string.docstring, string.quoted.single.block
  settings: {fontStyle: italic, foreground: '#218B97'}
- scope: comment
  settings: {foreground: '#475359'}
- scope: string
  settings: {foreground: '#74a9e3'}
- scope: meta.property-value.css string.quoted.double.css
  settings: {foreground: '#aded3e'}
- scope: 'meta.property-list.css meta.property-value.css string.quoted.double.css
    punctuation.definition.string.begin.css , meta.property-list.css meta.property-value.css
    string.quoted.double.css punctuation.definition.string.end.css '
  settings: {foreground: '#74a9e3'}
- name: String Quotes
