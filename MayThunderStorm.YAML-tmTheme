# [PackageDev] target_format: plist, ext: tmTheme
name: May Thunderstorm Theme
settings:
- settings: {activeGuide: '#1b2735', background: '#151718', bracketContentsOptions: underline,
    caret: '#ffe792', findHighlight: '#4FA5C7', foreground: '#c0c2c1', guide: '#71787c40',
    gutter: '#0D1011', gutterForeground: '#c0c2c170', inactiveSelection: '#2F6277',
    invisibles: '#F3FFB51A', lineHighlight: '#101112', selection: '#4FA5C7', selectionForeground: '#fefefe',
